    <script>
        // Add active tag to the page nav
        jQuery(document).ready(function($){
            // Get current path and find target link
            const path = window.location.pathname
            
            // Account for home page with empty path
            if ( path == '' ) {
                path = '/';
            }
            
            let target;
            if (path == "/") target = $('.loc1');
                else target = $('a[href="'+path+'"]');
                
            // Add active class to target link
            target.addClass('active');
        });

        // Handle the mode switching buttons
        // init sweetalert
        const Toast = Swal.mixin({
            toast: true,
            position: 'bottom-end',
            showConfirmButton: false,
            timer: 3000
        });

        // change mode to killer
        document.getElementById('modeKiller').addEventListener('click', e => {
            Toast.fire({
                icon: 'success',
                title: 'Changed stats mode to Killer.'
            })
            changeStatsMode("killer")
        });
        // change mode to killed
        document.getElementById('modeKilled').addEventListener('click', e => {
            Toast.fire({
                icon: 'success',
                title: 'Changed stats mode to Killed.'
            })
            changeStatsMode("killed")
        });

        // tell overwolf to change the mode
        function changeStatsMode(newMode){
            window.parent.postMessage(
                { mode: newMode }, 
            "overwolf-extension://mbpaeeolbbceleoodhaepaccaagapppmhojbcmed"
            ); 
        }
    </script>